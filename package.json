    "preview:ui-components": "npm --prefix ./apps/zeni-ui-components run preview",
    "dev:build": "run-p dev-app:*",
    "dev:run": "run-p preview:*",
    "build:main": "npm --prefix ./main-app run build",
    "build:ui-components": "npm --prefix ./apps/zeni-ui-components run build"
