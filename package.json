{
  "name": "zeni-guy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "apps/*",
    "main-app"
  ],
  "scripts": {
    "install:deps": "yarn install --check-files",
    "postinstall:deps": "yarn --cwd ./main-app build && yarn --cwd ./apps/zeni-ui-components build",
    "dev-app:main": "yarn --cwd ./main-app dev",
    "dev-app:ui-components": "yarn --cwd ./apps/zeni-ui-components build:watch",
    "preview:ui-components": "yarn --cwd ./apps/zeni-ui-components preview",
    "dev:init": "run-p dev-app:*",
    "dev": "run-p preview:*",
    "build:main": "yarn --cwd ./main-app build",
    "build:ui-components": "yarn --cwd ./apps/zeni-ui-components build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}